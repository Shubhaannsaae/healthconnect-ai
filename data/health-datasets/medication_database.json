          {
            "drug": "lithium",
            "severity": "moderate",
            "effect": "increased lithium levels"
          }
        ],
        "monitoring": [
          "blood pressure",
          "serum creatinine",
          "potassium levels",
          "BUN"
        ],
        "pregnancy_category": "D",
        "lactation": "compatible"
