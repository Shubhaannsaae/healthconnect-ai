      {
        "term": "Hypertension",
        "definition": "A condition in which the blood pressure in the arteries is persistently elevated above normal ranges (≥130/80 mmHg)",
        "category": "condition",
        "synonyms": ["high blood pressure", "arterial hypertension"],
        "related_terms": ["systolic pressure", "diastolic pressure", "blood pressure"],
        "icd10_code": "I10",
        "severity_levels": ["stage 1", "stage 2", "hypertensive crisis"],
        "normal_ranges": {
          "systolic": "< 120 mmHg",
          "diastolic": "< 80 mmHg"
        }
      },
      {
        "term": "Diabetes Mellitus",
