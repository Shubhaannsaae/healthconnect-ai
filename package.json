{
    "name": "healthconnect-ai-frontend",
    "version": "1.0.0",
    "private": true,
    "description": "Advanced AI-powered healthcare platform frontend",
    "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "lint:fix": "next lint --fix",
      "type-check": "tsc --noEmit",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "analyze": "cross-env ANALYZE=true next build",
      "clean": "rimraf .next out dist",
      "prepare": "husky install"
    },
    "dependencies": {
      "next": "^14.0.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "typescript": "^5.3.0",
      "@types/node": "^20.10.0",
      "@types/react": "^18.2.0",
      "@types/react-dom": "^18.2.0",
      "tailwindcss": "^3.3.0",
      "autoprefixer": "^10.4.0",
      "postcss": "^8.4.0",
      "clsx": "^2.0.0",
      "class-variance-authority": "^0.7.0",
      "lucide-react": "^0.294.0",
      "zustand": "^4.4.0",
      "react-hot-toast": "^2.4.0",
      "date-fns": "^2.30.0",
      "recharts": "^2.8.0",
      "@react-three/fiber": "^8.15.0",
      "@react-three/drei": "^9.88.0",
      "three": "^0.158.0",
      "@types/three": "^0.158.0",
      "aws-amplify": "^6.0.0",
      "socket.io-client": "^4.7.0",
      "@types/socket.io-client": "^3.0.0",
      "simple-peer": "^9.11.0",
      "@types/simple-peer": "^9.11.0"
    },
    "devDependencies": {
      "eslint": "^8.55.0",
      "eslint-config-next": "^14.0.0",
      "@typescript-eslint/eslint-plugin": "^6.13.0",
      "@typescript-eslint/parser": "^6.13.0",
      "eslint-plugin-react": "^7.33.0",
      "eslint-plugin-react-hooks": "^4.6.0",
      "eslint-plugin-jsx-a11y": "^6.8.0",
      "prettier": "^3.1.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-prettier": "^5.0.0",
      "husky": "^8.0.0",
      "lint-staged": "^15.2.0",
      "jest": "^29.7.0",
      "jest-environment-jsdom": "^29.7.0",
      "@testing-library/react": "^14.1.0",
      "@testing-library/jest-dom": "^6.1.0",
      "@testing-library/user-event": "^14.5.0",
      "cross-env": "^7.0.0",
      "rimraf": "^5.0.0",
      "@next/bundle-analyzer": "^14.0.0"
    },
    "engines": {
      "node": ">=18.0.0",
      "npm": ">=9.0.0"
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "lint-staged": {
      "*.{js,jsx,ts,tsx}": [
        "eslint --fix",
        "prettier --write"
      ],
      "*.{json,md,css}": [
        "prettier --write"
      ]
    },
    "keywords": [
      "healthcare",
      "telemedicine",
      "ai",
      "nextjs",
      "react",
      "typescript",
      "aws",
      "iot",
      "medical-devices"
    ],
    "author": "HealthConnect AI Team",
    "license": "MIT"
  }
  